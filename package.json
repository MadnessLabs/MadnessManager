{
  "name": "@madnesslabs/madnessmanager",
  "private": true,
  "version": "0.0.1",
  "description": "A project management tool by Madness Labs",
  "license": "MIT",
  "files": [
    "dist/"
  ],
  "scripts": {
    "android": "npm run env -- app && stencil build --cordova && cordova prepare android && cordova run android",
    "android:live": "npm run env -- applive && stencil build --cordova && cordova run android",
    "android:release": "npm run env -- applive && stencil build --cordova && node tasks/android.js",
    "ios": "npm run env -- app && stencil build --cordova && ionic cordova prepare ios --no-build",
    "ios:live": "npm run env -- applive && stencil build --cordova && ionic cordova prepare ios --no-build",
    "build": "if test \"$BRANCH\" = \"development\" ; then BUILD_ENV=app ; fi && npm run env -- ${BUILD_ENV:-applive} && stencil build --cordova",
    "build:dev": "npm run env -- dev && stencil build",
    "build:app": "npm run env -- app && stencil build",
    "build:live": " npm run env -- live && stencil build",
    "build:netlify": "npm run env -- netlify && stencil build",
    "build:prerender": "stencil build --prerender",
    "build:stats": "stencil build --stats",
    "deploy": "npm run deploy:dev",
    "deploy:dev": "npm run lint:app && npm run env -- dev && stencil build && firebase deploy --only hosting",
    "deploy:live": "npm run lint:app && npm run env -- live && stencil build && firebase deploy --only hosting -P live",
    "dev": "npm run env -- local && stencil build --dev --watch --serve",
    "dev:es5": "npm run env -- local && stencil build --dev --watch --serve --es5",
    "dev:share": "npm run env -- local && stencil build --dev --watch --serve --port 3245",
    "dev:android": "stencil build --cordova && sd concurrent \"cordova run -- --live-reload --port=3245\" ",
    "env": "node tasks/env.js",
    "git:prepush": "npm run lint:fix && stencil build",
    "ionic:build:before": "cd platforms/ios && pod install",
    "lint": "npm run lint:app",
    "lint:fix": "npm run lint:app:fix",
    "lint:app": "tslint -c ./tslint.json './src/**/*.{ts,tsx}'",
    "lint:app:fix": "tslint -c ./tslint.json './src/**/*.{ts,tsx}' --fix",
    "shell": "firebase functions:shell",
    "start": "npm run dev",
    "start:sw": "npm run env -- local && stencil build --dev --watch --serve --service-worker",
    "test": "stencil test --spec",
    "test:watch": "stencil test --spec --watch",
    "test:e2e": "stencil test --e2e",
    "v:minor": "node tasks/version.js minor",
    "v:patch": "node tasks/version.js patch"
  },
  "dependencies": {
    "@ionic-native/camera": "^5.0.0-beta.24",
    "@ionic-native/core": "^5.0.0-beta.24",
    "@ionic-native/facebook": "^5.0.0-beta.24",
    "@ionic-native/firebase-messaging": "^5.0.0-beta.24",
    "@ionic-native/google-plus": "^5.0.0-beta.24",
    "@ionic-native/ionic-webview": "^5.0.0-beta.24",
    "@ionic-native/social-sharing": "^5.0.0-beta.24",
    "@ionic-native/splash-screen": "^5.0.0-beta.24",
    "@ionic-native/stripe": "^5.0.0-beta.24",
    "@ionic-native/twitter-connect": "^5.0.0-beta.24",
    "@ionic/core": "4.0.0",
    "@ionic/pro": "^2.0.3",
    "@stencil/core": "0.17.0",
    "cordova-android": "^7.1.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-customurlscheme": "^4.3.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-facebook4": "^3.2.0",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-file-transfer": "^1.7.1",
    "cordova-plugin-firebase-messaging": "^1.2.1",
    "cordova-plugin-googleplus": "^5.3.2",
    "cordova-plugin-ionic": "^5.2.9",
    "cordova-plugin-ionic-keyboard": "^2.1.3",
    "cordova-plugin-ionic-webview": "^2.3.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-stripe": "^1.5.3",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-socialsharing": "^5.4.4",
    "cordova-support-android-plugin": "^1.0.1",
    "cordova-support-google-services": "^1.2.1",
    "cordova-universal-links-plugin": "git+https://github.com/walteram/cordova-universal-links-plugin.git",
    "cordova-universal-links-plugin-fix": "1.2.1",
    "es6-promise-plugin": "^4.2.2",
    "twitter-connect-plugin": "^0.6.0",
    "rxjs": "^6.3.2"
  },
  "devDependencies": {
    "@madnesslabs/enjin-components": "0.0.4",
    "@stencil/sass": "0.1.1",
    "@stencil/utils": "latest",
    "@types/jest": "23.3.7",
    "@types/stripe": "^5.0.25",
    "cleave.js": "^1.4.4",
    "firebase": "^5.5.1",
    "firebase-tools": "^6.0.0",
    "fs-extra": "^4.0.3",
    "jest": "23.6.0",
    "jest-cli": "23.6.0",
    "moment": "^2.22.2",
    "tslint": "^5.11.0",
    "tslint-ionic-rules": "0.0.14",
    "typescript": "^3.0.3",
    "workbox-build": "3.4.1",
    "xml2js": "^0.4.19"
  },
  "author": {
    "name": "Madness Labs",
    "url": "http://MadnessLabs.net",
    "email": "info@MadnessLabs.net"
  },
  "url": "https://madnessmanager.net/",
  "cordova": {
    "plugins": {
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-facebook4": {
        "APP_ID": "1249388151875664",
        "APP_NAME": "Madness Manager",
        "FACEBOOK_ANDROID_SDK_VERSION": "4.36.0"
      },
      "cordova-plugin-customurlscheme": {
        "URL_SCHEME": "net.madnessenjin.madnessmanager",
        "ANDROID_SCHEME": " ",
        "ANDROID_HOST": " ",
        "ANDROID_PATHPREFIX": "/"
      },
      "cordova-universal-links-plugin": {},
      "cordova-plugin-x-socialsharing": {
        "ANDROID_SUPPORT_V4_VERSION": "24.1.1+"
      },
      "cordova-plugin-camera": {},
      "cordova-plugin-firebase-messaging": {
        "FIREBASE_CORE_VERSION": "16.0.+",
        "FIREBASE_MESSAGING_VERSION": "17.3.+",
        "ANDROID_SUPPORT_VERSION": "27.+"
      },
      "cordova-support-google-services": {},
      "cordova-plugin-stripe": {}
    },
    "platforms": [
      "ios",
      "android"
    ]
  }
}
